package algoritmoestruturadedados;

public class main {

	public static void main(String[] args) {
		TadPilha pilha = new TadPilha(10);
		
		int valor = 0;
        while (!pilha.cheia()){
            if((valor % 2) > 0){
                pilha.empilha(valor);
            }
            valor++;
        }
//        c. Na sequência, exibir o conteúdo da pilha.
        pilha.imprime();
	}

}
